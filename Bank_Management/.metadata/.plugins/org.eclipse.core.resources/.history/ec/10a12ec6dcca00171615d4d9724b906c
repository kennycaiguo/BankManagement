import cx_Oracle
con=cx_Oracle.connect("Bank/12345678")
cur=con.cursor()
print(con.version)
cur.execute("create table customer (acc_no varchar2(10) primary key,name varchar2(20) not null, phone_no number not null,e-mail varchar2(40) not null,address varchar2(50) not null,cust_type varchar2(10) not null,bal number not null,security_money number not null")
cur.execute("create table balance (acc_no varchar2(10) primary key, bal number)")
cur.execute("create table login (username varchar2(10),password varchar2(15))")
cur.execute("create table tranx (acc_no varchar2(10) primary key,tranx_date date not null,amount number not null,type varchar2(2) not null)")
con.commit()
con.close()